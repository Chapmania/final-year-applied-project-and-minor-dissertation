      <template>
         Here you go.=3
      </template>
   </category>

    <category>
      <pattern>* TICKETS</pattern>
      <template>
         Here you go.=3
      </template>
   </category>

   <category>
      <pattern>* TICKET</pattern>
      <template>
         Here you go.=3
      </template>
   </category>

    <category>
      <pattern>TICKETS</pattern>
      <template>
         Here you go.=3
      </template>
   </category>

   <category>
      <pattern>TICKET</pattern>
      <template>
         Here you go.=3
      </template>
   </category>


    <!-- THANKS -->
