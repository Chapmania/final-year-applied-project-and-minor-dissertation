            <li>You're welcome.</li>
         </random>
      </template>
   </category>

   <!-- GOODBYE -->
   <category>
      <pattern>GOODBYE</pattern>
      <template>
         <random>
            <li>See you.</li>
            <li>Bye.</li>
         </random>
      </template>
   </category>

   <category>
      <pattern>BYE</pattern>
      <template>
