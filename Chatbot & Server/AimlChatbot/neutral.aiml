<aiml version="1.0.1" encoding="UTF-8">
    <!-- std-startup.xml -->

    <category>
        <pattern>HELLO</pattern>
        <template>
            Hi!
        </template>
    </category>

    <category>
        <pattern>HOW ARE YOU</pattern>
        <template>
            I'm okay
        </template>
    </category>

    <category>
        <pattern>MY NAME IS * CAN I SEE YOUR TICKET</pattern>
        <template>
             here it is <set name="usersName"><star/></set>!=3
        </template>
    </category>

    <category>
        <pattern>* MY NAME IS * CAN I SEE YOUR TICKET</pattern>
        <template>
            here it is <set name="usersName"><star index = "2"/></set>!=3
        </template>
    </category>

    <category>
      <pattern>TICKET PLEASE</pattern>
      <template>
         Here you go.=3
      </template>
   </category>

